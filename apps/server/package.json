{
  "name": "server",
  "packageManager": "yarn@3.6.1",
  "type": "module",
  "scripts": {
    "prod": "node ./dist/main.js",
    "dev": "ts-node ./src/main.ts",
    "dev:watch": "nodemon ./src/main.ts",
